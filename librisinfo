    // Funktion för att hämta in information från Libris från ett biblioteks sigel
    // ver 1

  	// Erroring in the built-in editor but it works... don't mind it =) /JS
  
    async function getLibraryinfos(branch, sigel) {
        const branchresponse = await fetch(`/cgi-bin/koha/getlibdata.pl?branch=${branch}&sigel=${branch}`);
        const sigelresponse = await fetch(`/cgi-bin/koha/getlibdata.pl?branch=${branch}&sigel=${sigel}`);
        const branchinfo = await branchresponse.json();
        const sigelinfo = await sigelresponse.json();
        const bibinfo = [branchinfo, sigelinfo];
        return bibinfo;
    }
